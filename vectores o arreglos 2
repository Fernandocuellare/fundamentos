// vectoresoarreglos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
int main ()
{
	int numeros[100];
    int suma=0;
	int n;
	cout<<"numero de elementos:";cin>>n;
	for(int i=0;i<n;i++){
		cout<<"numero de elementos["<<i<<"]:";
		cin>>numeros[i];
	}
	for(int i=0;i<n;i++)
	   suma=suma+numeros[i];

	 cout<<"la suma de los elementos del vector es:"<<suma<<endl;
   
   getch();
}
   
